{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/suzuki-shunsuke/ghtkn-go-sdk/ghtkn/internal/config/config",
  "$ref": "#/$defs/Config",
  "$defs": {
    "App": {
      "properties": {
        "name": {
          "type": "string"
        },
        "app_id": {
          "type": "integer"
        },
        "default": {
          "type": "boolean"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "name",
        "app_id"
      ]
    },
    "Config": {
      "properties": {
        "users": {
          "items": {
            "$ref": "#/$defs/User"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "users"
      ]
    },
    "User": {
      "properties": {
        "login": {
          "type": "string"
        },
        "apps": {
          "items": {
            "$ref": "#/$defs/App"
          },
          "type": "array"
        },
        "default": {
          "type": "boolean"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "login",
        "apps"
      ]
    }
  }
}
